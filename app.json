{
  "expo": {
    "name": "Just Another Sudoku",
    "slug": "just-another-sudoku",
    "displayName": "Just Another Sudoku",
    "scheme": "justanothersudoku",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "icon": "./assets/app-icon/icon.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "jsEngine": "jsc",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "icon": "./assets/app-icon/icon.png",
      "package": "com.averyhere.justanothersudoku",
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icon/adaptive-icon.png",
        "backgroundColor": "#171717"
      },
      "allowBackup": false,
      "edgeToEdgeEnabled": true
    },
    "ios": {
      "icon": "./assets/app-icon/ios-light.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.averyhere.justanothersudoku",
      "privacyManifests": {
        "NSPrivacyAccessedAPITypes": [
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": [
              "CA92.1"
            ]
          }
        ]
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "web": {
      "favicon": "./assets/app-icon/icon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-localization",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "image": {
            "light": "./assets/app-icons/splash-icon-light.png",
            "dark": "./assets/app-icons/splash-icon-dark.png"
          },
          "imageWidth": 300,
          "resizeMode": "contain",
          "backgroundColor": "#171717",
          "ios": {
            "icon": {
              "light": "./assets/app-icon/ios-light.png",
              "dark": "./assets/app-icon/ios-dark.png",
              "tinted": "./assets/app-icon/ios-tinted.png"
            }
          }
        }
      ],
      [
        "react-native-edge-to-edge",
        {
          "android": {
            "parentTheme": "Light",
            "enforceNavigationBarContrast": false
          }
        }
      ],
      "expo-router"
    ],
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    },
    "extra": {
      "ignite": {
        "version": "11.1.0"
      },
      "router": {},
      "eas": {
        "projectId": "ac2242e8-6900-47f3-8e9e-29d3df7075ba"
      }
    },
    "owner": "averyh3r3"
  }
}
